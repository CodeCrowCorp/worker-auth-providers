{
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "declaration": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src"],
  "exclude": [
      "example",
      "example-with-nextjs"
  ]
}